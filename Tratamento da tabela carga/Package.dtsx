    <DTS:Executable
      DTS:refId="Package\Tratar Pedidos"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Tarefa Executar SQL"
      DTS:DTSID="{4E276C23-28EA-43F3-9870-AFA70A0D810D}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Tratar Pedidos"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{4F2E1EE3-B5B2-4B48-87B0-584157F74F04}"
          SQLTask:SqlStatementSource="DECLARE @PedidoID INT, &#xA;&#x9;&#x9;@QuantidadePedido INT,&#xA;&#x9;&#x9;@ProdutoID INT,  &#xA;&#x9;&#x9;@QuantidadeEstoque INT,&#xA;&#x9;&#x9;@Total money;&#xA;&#xA;DECLARE pedidos_cursor CURSOR FOR&#xA;SELECT TM.&quot;id-order&quot;, SUM(TIP.&quot;quantity&quot;) AS QuantidadePedido, TIP.&quot;id-product&quot;, TP.total&#xA;FROM ItemPedido TIP &#xA;INNER JOIN Movimento TM ON TIP.&quot;id-order&quot; = TM.&quot;id-order&quot; &#xA;INNER JOIN Pedido    TP ON TP.&quot;id-order&quot;  = TM.&quot;id-order&quot; &#xA;WHERE TM.status &lt;&gt; 'Shipped'&#xA;GROUP BY TM.&quot;id-order&quot;, TIP.&quot;id-product&quot;, TP.total ORDER BY TP.total DESC;&#xA;&#xA;OPEN pedidos_cursor;&#xA;FETCH NEXT FROM pedidos_cursor INTO @PedidoID, @QuantidadePedido, @ProdutoID, @Total;&#xA;&#xA;WHILE @@FETCH_STATUS = 0&#xA;BEGIN&#xA;&#xA;    IF EXISTS &#xA;&#x9;&#x9;(&#xA;&#x9;&#x9;&#x9;SELECT 1 FROM Estoque&#xA;&#x9;&#x9;&#x9;WHERE &quot;id-product&quot; = @ProdutoID AND &quot;quantity&quot; &lt; @QuantidadePedido&#xA;    &#x9;)&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;UPDATE Movimento&#xA;&#x9;&#x9;&#x9;SET &quot;status&quot; = 'On Hold'&#xA;&#x9;&#x9;&#x9;WHERE &quot;id-order&quot; = @PedidoID;&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;UPDATE Comprar&#xA;&#x9;&#x9;&#x9;SET &quot;to-buy-quantity&quot; = &quot;to-buy-quantity&quot; + @QuantidadePedido&#xA;&#x9;&#x9;&#x9;WHERE &quot;id-product&quot; = @ProdutoID;&#xA;&#x9;&#x9;END&#xA;&#x9;ELSE&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;UPDATE Movimento&#xA;&#x9;&#x9;&#x9;SET &quot;status&quot; = 'Shipped', &quot;compleation-date&quot; = GETDATE()&#xA;&#x9;&#x9;&#x9;WHERE &quot;id-order&quot; = @PedidoID;&#xA;&#xA;&#x9;&#x9;&#x9;UPDATE Estoque&#xA;&#x9;&#x9;&#x9;SET &quot;quantity&quot; = &quot;quantity&quot; - @QuantidadePedido&#xA;&#x9;&#x9;&#x9;WHERE &quot;id-product&quot; = @ProdutoID;&#xA;&#x9;&#x9;END&#xA;&#x9;&#x9;&#xA;    FETCH NEXT FROM pedidos_cursor INTO @PedidoID, @QuantidadePedido, @ProdutoID, @Total;&#xA;END;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
