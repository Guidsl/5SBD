        <SQLTask:SqlTaskData
          SQLTask:Connection="{4F2E1EE3-B5B2-4B48-87B0-584157F74F04}"
          SQLTask:SqlStatementSource="INSERT INTO Estoque (&quot;id-product&quot;)&#xA;SELECT TP.&quot;id-product&quot; from Produto TP&#xA;LEFT JOIN Estoque TE ON TE.&quot;id-product&quot; = TP.&quot;id-product&quot; &#xA;WHERE TE.&quot;id-product&quot; IS NULL;&#xA;&#xA;INSERT INTO Comprar (&quot;id-product&quot;)&#xA;SELECT TP.&quot;id-product&quot; from Produto TP&#xA;LEFT JOIN Comprar TC ON TC.&quot;id-product&quot; = TP.&quot;id-product&quot; &#xA;WHERE TC.&quot;id-product&quot; IS NULL;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
